{
    "Spring": {
        "Cloud": {
            "Config": {
                "Enabled": false
            }
        }
    },
    "Eureka": {
        "Instance": {
            "IpAddress": "host.docker.internal",
            "PreferIpAddress": true
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "Steeltoe": "Information"
            }
        }
    }
}